{
  "description": "EcoSurf",
  "manifest_version": 2,
  "name": "EcoSurf",
  "version": "2.0",
  "homepage_url": "https://github.com/hugolhld/SI-Extension",
  "icons" : {
    "48": "icons/logo-48.png"
  },
  "permissions": [
    "activeTab","tabs","storage","<all_urls>"
  ],
  "background": {
    "scripts": ["background.js"]
  },
   "devtools_page": "EcoSurf-Analysis.html",
   "browser_action": {
    "default_icon": "icons/logo-48.png",
    "default_title": "EcoSurf",
    "default_popup": "menu.html"
  },
  "default_locale": "en"
}



